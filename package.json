{
  "name": "aurora",
  "version": "0.0.1",
  "description": "An artificial intelligence assistant, powered by ChatGPT.",
  "author": "leecervan904",
  "license": "ISC",
  "homepage": "https://github.com/leecervan904/aurora#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leecervan904/aurora.git"
  },
  "bugs": {
    "url": "https://github.com/leecervan904/aurora/issues"
  },
  "keywords": [
    "ai",
    "ChatGPT"
  ],
  "scripts": {
    "dev:web": "pnpm --filter @aurora/client-web dev",
    "dev:extension": "pnpm --filter @aurora/client-extension def",
    "dev:main": "pnpm --filter @aurora/www-main start:dev",
    "commit": "cz",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@antfu/eslint-config-react": "^0.39.5",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/estree": "^1.0.1",
    "commitizen": "^4.3.0",
    "eslint": "^8.42.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.8",
    "typescript": "~5.0.4"
  },
  "resolutions": {
    "@types/react": "^18.2.7",
    "@types/estree": "^1.0.1"
  },
  "lint-staged": {
    "*": "eslint . --fix"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}

{
  "name": "aurora",
  "version": "0.0.1",
  "description": "An artificial intelligence assistant, powered by ChatGPT.",
  "main": "index.js",
  "scripts": {
    "commit": "cz",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leecervan904/aurora.git"
  },
  "keywords": [
    "ai",
    "ChatGPT"
  ],
  "author": "leecervan904",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leecervan904/aurora/issues"
  },
  "homepage": "https://github.com/leecervan904/aurora#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "commitizen": "^4.3.0",
    "eslint": "^8.42.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.8"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.{scss,less,styl,html}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
